{
  "name": "server",
  "version": "1.3.0",
  "description": "Papriika.com web server",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/papriika/papriika.com.git"
  },
  "homepage": ".",
  "bugs": {
    "url": "https://github.com/papriika/papriika.com/issues/new"
  },
  "author": "Guy Lepage <guylepage3@gmail.com> (http://lepage.cc)",
  "contributors": {
    "url": "https://github.com/papriika/papriika.com/graphs/contributors"
  },
  "private": true,
  "main": "server.js",
  "keywords": [
    "express",
    "server"
  ],
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server",
    "server": "nodemon server",
    "client": "npm start --prefix client",
    "client-build": "npm run build --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^6.2.0",
    "express": "^4.17.1",
    "heroku-ssl-redirect": "0.1.1",
    "morgan": "^1.10.0",
    "nodemailer": "6.6.2",
    "npm-check-updates": "11.8.2",
    "request": "^2.88.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.12"
  }
}
